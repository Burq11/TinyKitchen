{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/index.js"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/index.html"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist"
        }
      }
    ],
    "devCommand": "npm run dev --prefix frontend"
  }